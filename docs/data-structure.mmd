---
title: Auro Data Structure
---
classDiagram
    class Asset
    Asset : +TEXT id [PK , NOT NULL]
    Asset : +TEXT name [NOT NULL]
    Asset : +TEXT symbol [NOT NULL]
    Asset : +TEXT kind [ NOT NULL CHECK (kind IN ('stablecoin', 'cryptocoin', 'nft')) ] 
    Asset : +Date createdAt [DEFAULT CURRENT_TIMESTAMP]
    Asset : +Date updatedAt [DEFAULT CURRENT_TIMESTAMP]
    class Fiat
    Fiat : +Int id [PK , NOT NULL AUTOINCREMENT]
    Fiat : +String name [NOT NULL]
    Fiat : +String symbol [NOT NULL]
    Fiat : +Date createdAt [DEFAULT CURRENT_TIMESTAMP]
    Fiat : +Date updatedAt [DEFAULT CURRENT_TIMESTAMP]
    class Sys_tracker
    Sys_tracker : +Int id [PK , NOT NULL AUTOINCREMENT]
    Sys_tracker : +String name [NOT NULL]
    Sys_tracker : +Date last_updated_at [NOT NULL]
    class FiatRamp
    FiatRamp : +String id [PK , NOT NULL]
    FiatRamp : +Int fiat_id [NOT NULL]
    FiatRamp : +REAL fiat_amount [NOT NULL]
    FiatRamp : +Date ramp_date [NOT NULL DEFAULT CURRENT_DATE]
    FiatRamp : +String via_exchange [NOT NULL]
    FiatRamp : +String kind [ NOT NULL CHECK (kind IN ('deposit', 'withdraw')) ] 
    FiatRamp : +TIMESTAMP createdAt [DEFAULT CURRENT_TIMESTAMP]
    FiatRamp : +TIMESTAMP updatedAt [DEFAULT CURRENT_TIMESTAMP]
    
    
    
